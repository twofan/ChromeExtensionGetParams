{
  "manifest_version": 2,

  "name": "URL Parameter Copy Plugin",
  "description": "This extension will help copying URL Parameter",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "clipboardWrite",
    "clipboardRead"
  ],
  "background": {
    "scripts": ["eventPage.js"],
    "persistent": false
  }
}
